package br.com.ad.spring.data.orm;

import javax.persistence.Embeddable;

@Embeddable
public class Endereco {
	
	private Integer numero;
	private String rua;
	private String bairro;
	private String cidade;
	private String estado;
	
	public Endereco() {
	
	}

	public Endereco(Integer numero, String rua, String bairro, String cidade, String estado) {
		this.numero = numero;
		this.rua = rua;
		this.bairro = bairro;
		this.cidade = cidade;
		this.estado = estado;
	}
	
	
}
